<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
  <meta>
    <author>Jamie Matthews</author>
    <description>YQL table for Last.fm Album.getInfo API method. Get the metadata for an album on Last.fm using the album name or a musicbrainz id. See playlist.fetch on how to get the album playlist.</description>
    <documentationURL>http://www.last.fm/api/show?service=290</documentationURL>
  </meta>
  <bindings>
    <select itemPath="" produces="XML">
      <urls>
        <url>http://ws.audioscrobbler.com/2.0/?method=album.getinfo</url>
      </urls>
      <inputs>
        <key id="event" type="xs:string" paramType="query" required="false" />
        <key id="artist" type="xs:string" paramType="query" required="false" />
        <key id="album" type="xs:string" paramType="query" required="false" />
        <key id="mbid" type="xs:string" paramType="query" required="false" />
        <key id="username" type="xs:string" paramType="query" required="false" />
        <key id="lang" type="xs:string" paramType="query" required="false" />
        <key id="api_key" type="xs:string" paramType="query" required="true" />
      </inputs>
    </select>
  </bindings>
</table>
